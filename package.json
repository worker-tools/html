{
  "name": "@worker-tools/html",
  "version": "1.3.1",
  "description": "HTML templating and streaming response library for worker environments such as Cloudflare Workers",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "files": [
    "src",
    "vendor",
    "*.js*",
    "*.d.ts*"
  ],
  "types": "./index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf *.d.ts *.js *.js.map *.d.ts.map vendor",
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "tsc -d",
    "prepack": "npm run clean && npm run build"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "whatwg-stream-to-async-iter": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worker-tools/html.git"
  },
  "bugs": {
    "url": "https://github.com/worker-tools/html/issues"
  },
  "homepage": "https://github.com/worker-tools/html#readme",
  "keywords": [
    "html",
    "service-worker",
    "templating",
    "workers",
    "streams",
    "response",
    "whatwg-streams",
    "fetch-api"
  ]
}
